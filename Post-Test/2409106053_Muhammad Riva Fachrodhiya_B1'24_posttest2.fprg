    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Nama, Alamat, Pekerjaan" type="String" array="False" size=""/>
            <declare name="BeratBadan, TinggiBadan, JumlahNumerik" type="Real" array="False" size=""/>
            <declare name="Umur" type="Integer" array="False" size=""/>
            <declare name="VerifikasiData" type="Boolean" array="False" size=""/>
            <declare name="x" type="String" array="False" size=""/>
            <output expression="&quot;Nama Anda&quot;" newline="True"/>
            <input variable="Nama"/>
            <output expression="&quot;Umur Anda&quot;" newline="True"/>
            <input variable="Umur"/>
            <output expression="&quot;Alamat Anda&quot;" newline="True"/>
            <input variable="Alamat"/>
            <output expression="&quot;Berat Badan Anda&quot;" newline="True"/>
            <input variable="BeratBadan"/>
            <output expression="&quot;Tinggi Badan Anda&quot;" newline="False"/>
            <input variable="TinggiBadan"/>
            <output expression="&quot;Pekerjaan Anda Apa&quot;" newline="True"/>
            <input variable="Pekerjaan"/>
            <output expression="&quot;Apakah Data Anda Sudah Benar? (True/False)&quot;" newline="True"/>
            <input variable="VerifikasiData"/>
            <if expression="VerifikasiData = True">
                <then>
                    <assign variable="x" expression="&quot;Sudah&quot;"/>
                </then>
                <else>
                    <assign variable="x" expression="&quot;Belum&quot;"/>
                </else>
            </if>
            <assign variable="JumlahNumerik" expression="Umur + TinggiBadan + BeratBadan"/>
            <output expression="&quot;===================================&quot;" newline="True"/>
            <output expression="&quot;Bio Data Anda&quot;" newline="True"/>
            <output expression="&quot;===================================&quot;" newline="True"/>
            <output expression="Nama" newline="True"/>
            <output expression="Umur" newline="True"/>
            <output expression="Alamat" newline="True"/>
            <output expression="BeratBadan" newline="True"/>
            <output expression="TinggiBadan" newline="True"/>
            <output expression="Pekerjaan" newline="True"/>
            <output expression="x" newline="True"/>
            <output expression="&quot;===================================&quot;" newline="True"/>
            <output expression="JumlahNumerik" newline="True"/>
            <output expression="&quot;===================================&quot;" newline="True"/>
        </body>
